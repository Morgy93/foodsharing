{
  "name": "foodsharing-chat-server",
  "description": "foodsharing socket.io chat server",
  "repository": "git@gitlab.com:foodsharing-dev/foodsharing.git",
  "private": true,
  "version": "0.1.0",
  "engines": {
    "node": ">=6.5.0"
  },
  "scripts": {
    "lint": "eslint -c package.json .",
    "fix": "eslint -c package.json --fix .",
