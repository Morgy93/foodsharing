{
  "license": "proprietary",
  "name": "foodsharing/foodsharing",
  "description": "Network for foodsavers and foodsharers",
  "config": {
    "platform": {
      "php": "8.1"
    },
    "sort-packages": true,
    "allow-plugins": {
      "php-http/discovery": false
    }
  },
  "require-dev": {
    "codeception/codeception": "4.2.*",
    "codeception/module-asserts": "2.0.*",
    "codeception/module-cli": "2.0.*",
    "codeception/module-db": "2.0.*",
    "codeception/module-filesystem": "2.0.*",
    "codeception/module-phpbrowser": "2.0.*",
    "codeception/module-rest": "2.0.*",
    "codeception/module-sequence": "2.0.*",
    "codeception/module-symfony": "2.1.*",
    "codeception/module-webdriver": "2.0.*",
    "fakerphp/faker": "1.21.*",
    "friendsofphp/php-cs-fixer": "3.12.*",
    "php-parallel-lint/php-console-highlighter": "1.0.*",
    "php-parallel-lint/php-parallel-lint": "1.3.*",
    "phpstan/phpstan": "1.9.*",
    "phpunit/phpunit": "9.5.*",
    "roave/security-advisories": "dev-latest",
    "robmorgan/phinx": "0.12.12",
    "sebastian/diff": "4.0.*"
  },
  "require": {
    "ext-dom": "*",
    "ext-fileinfo": "*",
    "ext-gd": "*",
    "ext-imagick": "*",
    "ext-imap": "*",
    "ext-intl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-mysqli": "*",
    "ext-openssl": "*",
    "ext-pdo": "*",
    "ext-redis": "*",
    "ext-simplexml": "*",
    "ext-zlib": "*",
    "ddeboer/imap": "1.15.*",
    "doctrine/annotations": "1.14.*",
    "doctrine/cache": "2.2.*",
    "erusev/parsedown": "1.7.*",
    "exercise/htmlpurifier-bundle": "4.1.*",
    "filp/whoops": "2.14.*",
    "flagception/flagception-bundle": "^4.3",
    "fpdo/fluentpdo": "2.2.*",
    "friendsofsymfony/rest-bundle": "3.4.*",
    "html2text/html2text": "4.3.*",
    "influxdb/influxdb-php": "1.15.*",
    "jms/serializer-bundle": "5.2.*",
    "jsvrcek/ics": "0.8.4",
    "minishlink/web-push": "8.0.*",
    "mobiledetect/mobiledetectlib": "2.8.*",
    "nelmio/api-doc-bundle": "4.11.*",
    "nesbot/carbon": "2.66.*",
    "sensio/framework-extra-bundle": "6.2.*",
    "sentry/sentry-symfony": "4.5.*",
    "setasign/fpdi": "2.3.*",
    "symfony/asset": "5.4.*",
    "symfony/config": "5.4.*",
    "symfony/console": "5.4.*",
    "symfony/dependency-injection": "5.4.*",
    "symfony/error-handler": "5.4.*",
    "symfony/expression-language": "5.4.*",
    "symfony/form": "5.4.*",
    "symfony/framework-bundle": "5.4.*",
    "symfony/http-foundation": "5.4.*",
    "symfony/http-kernel": "5.4.*",
    "symfony/mailer": "5.4.*",
    "symfony/translation": "5.4.*",
    "symfony/twig-bridge": "5.4.*",
    "symfony/twig-bundle": "5.4.*",
    "symfony/validator": "5.4.*",
    "symfony/yaml": "5.4.*",
    "tecnickcom/tcpdf": "6.5.*",
    "twig/intl-extra": "3.5.*",
    "twig/twig": "3.5.*",
    "voku/bounce-mail-handler": "6.0.*"
  },
  "autoload": {
    "psr-4": {
      "Foodsharing\\": "src/",
      "Flourish\\": "src/Lib/Flourish"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Foodsharing\\": "tests/"
    }
  },
  "extra": {
    "public-dir": "assets",
    "symfony": {
      "require": "5.4.*"
    }
  }
}
