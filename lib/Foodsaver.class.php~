			'.$this->intval($data['has_children']).',
			'.$this->strval($data['name']).',
			'.$this->strval($data['email']).',
			'.$this->strval($data['email_pass']).',
			'.$this->strval($data['email_name']).'
			)');
		
		
			if(isset($data['foodsaver']) && is_array($data['foodsaver']))
