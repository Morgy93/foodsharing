	public function get_send_email()
	{
		$out = $this->q('
			SELECT 	 	
			`id`,
			`foodsaver_id`,
			`complete`,
			`name`,
			`message`,
			`zeit`,
			`recip`,
			`attach`
					
			FROM 		`'.PREFIX.'send_email`
			ORDER BY `name`');
		
		return $out;
	}
					
	public function getBasics_send_email()
	{
		return $this->q('
			SELECT 	 	`id`,
						`name`
					
			FROM 		`'.PREFIX.'send_email`
			ORDER BY `name`');
	}
					
	public function getOne_send_email($id)
	{
		$out = $this->qRow('
			SELECT 	 	
			`id`,
			`foodsaver_id`,
			`complete`,
			`name`,
			`message`,
			`zeit`,
			`recip`,
			`attach`
					
			FROM 		`'.PREFIX.'send_email`
					
			WHERE 		`id` = ' . $this->intval($id));
		
		
				
		return $out;
	}
	
